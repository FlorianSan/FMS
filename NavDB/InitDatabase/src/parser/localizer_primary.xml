<?xml version="1.0" encoding="UTF-8"?>
<jsp:schema xmlns:jsp="http://jsapar.tigris.org/JSaParSchema/1.0" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
            xsi:schemaLocation="http://jsapar.tigris.org/JSaParSchema/1.0 ../xml/schema/JSaParSchema.xsd ">
    <jsp:fixedwidthschema lineseparator="\n" >
        <jsp:line occurs="*" trimfillcharacters="false" linetype="primary">
            <jsp:cell name="S/T" length="1" alignment="left" mandatory="true"/>
            <jsp:cell name="CUST/AREA" length="3" alignment="left" mandatory="true"/>
            <jsp:cell name="SEC CODE" length="1" alignment="left" mandatory="true"/>
            <jsp:cell name="SPACE" length="1" alignment="left" />
            <jsp:cell name="ARPT IDENT" length="4" alignment="left"/>
            <jsp:cell name="ICAO CODE" length="2" alignment="left"/>
            <jsp:cell name="SUB CODE" length="1" alignment="left"/>
            <jsp:cell name="LOC IDENT" length="4" alignment="left"/>
            <jsp:cell name="CAT" length="1" alignment="left"/>
            <jsp:cell name="SPACE2" length="3" alignment="left"/>
            <jsp:cell name="CONT NR" length="1" alignment="left" mandatory="true" />
            <jsp:cell name="FREQ" length="5" alignment="left" />
            <jsp:cell name="RUNWAY IDENT" length="5" alignment="left"/>
            <jsp:cell name="LOC LATITUDE" length="9" alignment="left"/>
            <jsp:cell name="LOC LONGITUDE" length="10" alignment="left"/>
            <jsp:cell name="LOC BRG" length="4" alignment="left"/>
            <jsp:cell name="GS LATITUDE" length="9" alignment="left"/>
            <jsp:cell name="GS LONGITUDE" length="10" alignment="left"/>
            <jsp:cell name="LOC FR RW END" length="4" alignment="left"/>
            <jsp:cell name="+/-/@" length="1" alignment="left"/>
            <jsp:cell name="GS FR RW THRES" length="4" alignment="left"/>
            <jsp:cell name="LOC WIDTH" length="4" alignment="left"/>
            <jsp:cell name="GS ANGLE" length="3" alignment="left"/>
            <jsp:cell name="STA DECL" length="4" alignment="left"/>
            <jsp:cell name="TCH" length="2" alignment="left"/>
            <jsp:cell name="GS ELEV" length="5" alignment="left"/>
            <jsp:cell name="SUPPORT FACILITY" length="4" alignment="left"/>
            <jsp:cell name="ICAO CODE FACILITY" length="2" alignment="left"/>
            <jsp:cell name="SEC CODE FACILITY" length="1" alignment="left"/>
            <jsp:cell name="SUB CODE FACILITY" length="1" alignment="left"/>
            <jsp:cell name="RESERVED" length="13" alignment="left"/>
            <jsp:cell name="FILE RECORD NUMBER" length="5" alignment="left"/>
            <jsp:cell name="CYCLE" length="4" alignment="left"/>
     </jsp:line>
    </jsp:fixedwidthschema>
</jsp:schema>

