<?xml version="1.0" encoding="UTF-8"?>
<jsp:schema xmlns:jsp="http://jsapar.tigris.org/JSaParSchema/1.0" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
            xsi:schemaLocation="http://jsapar.tigris.org/JSaParSchema/1.0 ../xml/schema/JSaParSchema.xsd ">
    <jsp:fixedwidthschema lineseparator="\n" >
        <jsp:line occurs="*" trimfillcharacters="false" linetype="simulation">
            <jsp:cell name="S/T" length="1" alignment="left" mandatory="true"/>
            <jsp:cell name="CUST/AREA" length="3" alignment="left" mandatory="true"/>
            <jsp:cell name="SEC CODE" length="1" alignment="left" mandatory="true"/>
            <jsp:cell name="SPACE" length="1" alignment="left" />
            <jsp:cell name="ARPT IDENT" length="4" alignment="left"/>
            <jsp:cell name="ICAO CODE" length="2" alignment="left"/>
            <jsp:cell name="SUB CODE" length="1" alignment="left"/>
            <jsp:cell name="LOC IDENT" length="4" alignment="left"/>
            <jsp:cell name="CAT" length="1" alignment="left"/>
            <jsp:cell name="SPACE2" length="3" alignment="left"/>
            <jsp:cell name="CONT NR" length="1" alignment="left" mandatory="true" />
            <jsp:cell name="APPL TYPE" length="1" alignment="left" mandatory="true" />
            <jsp:cell name="SPACE3" length="4" alignment="left" />
            <jsp:cell name="FAC CHAR" length="5" alignment="left" />
            <jsp:cell name="SPACE4" length="19" alignment="left" />
            <jsp:cell name="TRUE BEARING" length="4" alignment="left" />
            <jsp:cell name="SOURCE" length="1" alignment="left" />
            <jsp:cell name="SPACE5" length="31" alignment="left" />
            <jsp:cell name="GS BEAM WIDTH" length="3" alignment="left" />
            <jsp:cell name="APP IDENT 1" length="6" alignment="left" />
            <jsp:cell name="APP IDENT 2" length="6" alignment="left" />
            <jsp:cell name="APP IDENT 3" length="6" alignment="left" />
            <jsp:cell name="APP IDENT 4" length="6" alignment="left" />
            <jsp:cell name="APP IDENT 5" length="6" alignment="left" />
            <jsp:cell name="SPACE 6" length="3" alignment="left" />
            <jsp:cell name="FILE RECORD NUMBER" length="5" alignment="left"/>
            <jsp:cell name="CYCLE" length="4" alignment="left"/>
        </jsp:line>
    </jsp:fixedwidthschema>
</jsp:schema>

