<?xml version="1.0" encoding="UTF-8"?>
<jsp:schema xmlns:jsp="http://jsapar.tigris.org/JSaParSchema/1.0" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
            xsi:schemaLocation="http://jsapar.tigris.org/JSaParSchema/1.0 ../xml/schema/JSaParSchema.xsd ">
    <jsp:fixedwidthschema lineseparator="\n" >
        <jsp:line occurs="*" trimfillcharacters="false" linetype="limitation">
            <jsp:cell name="S/T" length="1" alignment="left" mandatory="true"/>
            <jsp:cell name="CUST/AREA" length="3" alignment="left" mandatory="true"/>
            <jsp:cell name="SEC CODE" length="1" alignment="left" mandatory="true"/>
            <jsp:cell name="SUB CODE" length="1" alignment="left" />
            <jsp:cell name="ARPT IDENT" length="4" alignment="left"/>
            <jsp:cell name="ICAO CODE" length="2" alignment="left"/>
            <jsp:cell name="SPACE" length="1" alignment="left"/>
            <jsp:cell name="VOR IDENT" length="4" alignment="left"/>
            <jsp:cell name="SPACE2" length="2" alignment="left"/>
            <jsp:cell name="ICAO CODE2" length="2" alignment="left"/>
            <jsp:cell name="CONT NR" length="1" alignment="left" mandatory="true" />
            <jsp:cell name="APPL TYPE" length="1" alignment="left" mandatory="true"/>
            <jsp:cell name="NLC" length="1" alignment="left"/>
            <jsp:cell name="CAI" length="1" alignment="left"/>
            <jsp:cell name="SEQ NR" length="2" alignment="left"/>
            <jsp:cell name="SECTOR" length="2" alignment="left"/>
            <jsp:cell name="DIST DESC" length="1" alignment="left"/>
            <jsp:cell name="DIST LIMIT" length="6" alignment="left"/>
            <jsp:cell name="ALT DESC" length="1" alignment="left"/>
            <jsp:cell name="ALT LIMIT" length="6" alignment="left"/>
            <jsp:cell name="SECTOR2" length="1" alignment="left"/>
            <jsp:cell name="DIST DESC2" length="1" alignment="left"/>
            <jsp:cell name="DIST LIMIT2" length="6" alignment="left"/>
            <jsp:cell name="ALT DESC2" length="1" alignment="left"/>
            <jsp:cell name="ALT LIMIT2" length="6" alignment="left"/>
            <jsp:cell name="SECTOR3" length="2" alignment="left"/>
            <jsp:cell name="DIST DESC3" length="1" alignment="left"/>
            <jsp:cell name="DIST LIMIT3" length="6" alignment="left"/>
            <jsp:cell name="ALT DESC3" length="1" alignment="left"/>
            <jsp:cell name="ALT LIMIT3" length="6" alignment="left"/>
            <jsp:cell name="SECTOR4" length="2" alignment="left"/>
            <jsp:cell name="DIST DESC4" length="1" alignment="left"/>
            <jsp:cell name="DIST LIMIT4" length="6" alignment="left"/>
            <jsp:cell name="ALT DESC4" length="1" alignment="left"/>
            <jsp:cell name="ALT LIMIT4" length="6" alignment="left"/>
            <jsp:cell name="SECTOR5" length="2" alignment="left"/>
            <jsp:cell name="DIST DESC5" length="1" alignment="left"/>
            <jsp:cell name="DIST LIMIT5" length="6" alignment="left"/>
            <jsp:cell name="ALT DESC5" length="1" alignment="left"/>
            <jsp:cell name="ALT LIMIT5" length="6" alignment="left"/>
            <jsp:cell name="SEQ IND" length="1" alignment="left"/>
            <jsp:cell name="SPACE2" length="15" alignment="left"/>
            <jsp:cell name="FILE RECORD NUMBER" length="5" alignment="left"/>
            <jsp:cell name="CYCLE" length="4" alignment="left"/>
        </jsp:line>
    </jsp:fixedwidthschema>
</jsp:schema>
