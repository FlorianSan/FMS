<?xml version="1.0" encoding="UTF-8"?>
<jsp:schema xmlns:jsp="http://jsapar.tigris.org/JSaParSchema/1.0" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
            xsi:schemaLocation="http://jsapar.tigris.org/JSaParSchema/1.0 ../xml/schema/JSaParSchema.xsd ">
    <jsp:fixedwidthschema lineseparator="\n" >
        <jsp:line occurs="*" trimfillcharacters="false" linetype="primary">
            <jsp:cell name="S/T" length="1" alignment="left" mandatory="true"/>
            <jsp:cell name="CUST/AREA" length="3" alignment="left" mandatory="true"/>
            <jsp:cell name="SEC CODE" length="1" alignment="left" mandatory="true"/>
            <jsp:cell name="SPACE" length="1" alignment="left" />
            <jsp:cell name="ARPT IDENT" length="4" alignment="left"/>
            <jsp:cell name="ICAO CODE" length="2" alignment="left"/>
            <jsp:cell name="SUB CODE" length="1" alignment="left"/>
            <jsp:cell name="RUNWAY IDENT" length="5" alignment="left"/>
            <jsp:cell name="SPACE2" length="3" alignment="left"/>
            <jsp:cell name="CONT NR" length="1" alignment="left" mandatory="true" />
            <jsp:cell name="RUNWAY LENGTH" length="5" alignment="left" />
            <jsp:cell name="RUNWAY BEARING" length="4" alignment="left"/>
            <jsp:cell name="SPACE3" length="1" alignment="left"/>
            <jsp:cell name="LATITUDE" length="9" alignment="left"/>
            <jsp:cell name="LONGITUDE" length="10" alignment="left"/>
            <jsp:cell name="RWY GRAD" length="5" alignment="left"/>
            <jsp:cell name="SPACE4" length="4" alignment="left"/>
            <jsp:cell name="ELLIPSOID HEIGHT" length="6" alignment="left"/>
            <jsp:cell name="LNDG THRES ELEV" length="5" alignment="left"/>
            <jsp:cell name="DSPLCD THR" length="4" alignment="left"/>
            <jsp:cell name="TCH" length="2" alignment="left"/>
            <jsp:cell name="WIDTH" length="3" alignment="left"/>
            <jsp:cell name="LOC GLS IDENT" length="4" alignment="left"/>
            <jsp:cell name="CAT/CLASS LOC GLS IDENT" length="4" alignment="left"/>
            <jsp:cell name="STOPWAY" length="4" alignment="left"/>
            <jsp:cell name="SEC LOC GLS IDENT" length="4" alignment="left"/>
            <jsp:cell name="CAT/CLASS SEC LOC GLS IDENT" length="4" alignment="left"/>
            <jsp:cell name="RESERVED" length="6" alignment="left"/>
            <jsp:cell name="RUNWAY DESCRIPTION" length="22" alignment="left"/>
            <jsp:cell name="FILE RECORD NUMBER" length="5" alignment="left"/>
            <jsp:cell name="CYCLE" length="4" alignment="left"/>
     </jsp:line>
    </jsp:fixedwidthschema>
</jsp:schema>

